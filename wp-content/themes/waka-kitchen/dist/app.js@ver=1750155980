Y_ANIMS,(function(){t.nodeInDOM.classList.remove(t.isHiddenClass)})),this.isTouch||(this.ticketClose.addEventListener("mouseenter",(function(){pc.emit(mc.SHOW_CLOSE_MOUSE)})),this.ticketClose.addEventListener("mousemove",(function(t){pc.emit(mc.MOVE_CLOSE_MOUSE,t)})),this.ticketClose.addEventListener("mouseleave",(function(t){pc.emit(mc.HIDE_CLOSE_MOUSE,t)}))),this.ticketClose.addEventListener("click",(function(){pc.emit(mc.START_HIDE_TICKET_ANIM)}))}},{key:"handleEvent",value:function(t){}},{key:"removeEvents",value:function(){pc.off(mc.START_SHOW_TICKET_ANIM,this.handleEventCallback),pc.off(mc.OPEN_MOB_MENU,this.handleEventCallback),pc.off(mc.START_HIDE_TICKET_ANIM,this.handleEventCallback),pc.off(mc.CLOSE_MOB_MENU,this.handleEventCallback),pc.off(mc.APP_INIT,this.handleEventCallback),pc.off(mc.SHOW_CLOSE_MOUSE,this.handleEventCallback),pc.off(mc.MOVE_CLOSE_MOUSE,this.handleEventCallback),pc.off(mc.HIDE_CLOSE_MOUSE,this.handleEventCallback),this.ticketClose.removeEventListener("mouseenter",this.handleEventCallback),this.ticketClose.removeEventListener("mousemove",this.handleEventCallback),this.ticketClose.removeEventListener("mouseleave",this.handleEventCallback),this.ticketClose.removeEventListener("click",this.handleEventCallback)}},{key:"destroy",value:function(){this.nodeInDOM&&(this.removeEvents(),this.nodeInDOM=null)}}],e&&v_(t.prototype,e),n&&v_(t,n),Object.defineProperty(t,"prototype",{writable:!1}),t;var t,e,n}();const x_=y_;var b_=n(714),S_=n.n(b_);function M_(t){return M_="function"==typeof Symbol&&"symbol"==typeof Symbol.iterator?function(t){return typeof t}:function(t){return t&&"function"==typeof Symbol&&t.constructor===Symbol&&t!==Symbol.prototype?"symbol":typeof t},M_(t)}function E_(){/*! regenerator-runtime -- Copyright (c) 2014-present, Facebook, Inc. -- license (MIT): https://github.com/facebook/regenerator/blob/main/LICENSE */E_=function(){return e};var t,e={},n=Object.prototype,i=n.hasOwnProperty,r=Object.defineProperty||function(t,e,n){t[e]=n